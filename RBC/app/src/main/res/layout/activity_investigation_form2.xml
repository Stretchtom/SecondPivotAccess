<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.tmutabazi.rbc.UI.InvestigationForm2"
    android:id="@+id/a">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Date of birth"
        android:id="@+id/textView77"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText12"
        android:layout_alignTop="@+id/textView77"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Gender"
        android:id="@+id/textView78"
        android:layout_below="@+id/editText12"
        android:layout_alignLeft="@+id/textView77"
        android:layout_alignStart="@+id/textView77"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />
    <RadioGroup
        android:id="@+id/housesWithHoles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/textView78"
        android:orientation="horizontal"
        android:layout_marginTop="55dp">




    <RadioButton
        android:id="@+id/radioMale"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="M"
        android:checked="true"
        android:layout_below="@+id/sName"
        android:layout_alignRight="@+id/scrollView"
        android:layout_alignEnd="@+id/scrollView"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <RadioButton
        android:id="@+id/radioFemale"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="F"
        android:checked="false"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:textColor="#ff000000"
        android:textSize="30dp" />
    </RadioGroup>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nationality"
        android:id="@+id/textView79"
        android:layout_alignBottom="@+id/textView78"
        android:layout_alignLeft="@+id/editText12"
        android:layout_alignStart="@+id/editText12"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_marginTop="10dp" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText13"
        android:layout_alignTop="@+id/housesWithHoles"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="ID number"
        android:id="@+id/textView80"
        android:layout_below="@+id/editText13"
        android:layout_alignLeft="@+id/textView78"
        android:layout_alignStart="@+id/textView78"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:id="@+id/idNumber"
        android:layout_alignTop="@+id/textView80"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Residential district:"
        android:id="@+id/textView81"
        android:layout_below="@+id/idNumber"
        android:layout_alignLeft="@+id/textView80"
        android:layout_alignStart="@+id/textView80"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <Spinner
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/residentialDistrict"
        android:layout_alignTop="@+id/textView81"
        android:layout_alignRight="@+id/idNumber"
        android:layout_alignEnd="@+id/idNumber"
        android:layout_marginRight="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Sector:"
        android:id="@+id/textView82"
        android:layout_below="@+id/residentialDistrict"
        android:layout_alignLeft="@+id/textView81"
        android:layout_alignStart="@+id/textView81"
        android:layout_marginTop="10dp"
        android:textColor="#ff010101"
        android:textSize="30dp" />

    <Spinner
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/sector"
        android:layout_alignTop="@+id/textView82"
        android:layout_alignRight="@+id/textView81"
        android:layout_alignEnd="@+id/textView81" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Cell:"
        android:id="@+id/textView83"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_alignBottom="@+id/sector"
        android:layout_alignRight="@+id/textView79"
        android:layout_alignEnd="@+id/textView79"
        android:layout_marginRight="30dp" />

    <Spinner
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/cell"
        android:layout_below="@+id/residentialDistrict"
        android:layout_toRightOf="@+id/textView79"
        android:layout_toEndOf="@+id/textView79"
        android:layout_marginTop="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Village"
        android:id="@+id/textView84"
        android:layout_below="@+id/sector"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText18"

        android:layout_alignTop="@+id/textView84"
        android:layout_alignLeft="@+id/sector"
        android:layout_alignStart="@+id/sector" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Pregnant?"
        android:id="@+id/textView85"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_alignBottom="@+id/editText18"
        android:layout_alignRight="@+id/textView83"
        android:layout_alignEnd="@+id/textView83" />
    <RadioGroup
        android:id="@+id/isPregnant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editText18"
        android:layout_toEndOf="@+id/textView85"
        android:layout_toRightOf="@+id/textView85"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp">



    <RadioButton
        android:id="@+id/radiopregnant1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes"
        android:checked="true"
        android:layout_below="@+id/sName"
        android:layout_alignRight="@+id/scrollView"
        android:layout_alignEnd="@+id/scrollView"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <RadioButton
        android:id="@+id/radiopregnant2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No"
        android:checked="false"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:textColor="#ff000000"
        android:textSize="30dp" />
    </RadioGroup>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Head of household name"
        android:id="@+id/textView86"
        android:layout_below="@+id/editText18"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/headOfHouseholdName"
        android:layout_alignTop="@+id/textView86"
        android:layout_toRightOf="@+id/textView85"
        android:layout_toEndOf="@+id/textView85" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Telephone mobile"
        android:id="@+id/textView87"
        android:layout_below="@+id/headOfHouseholdName"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:textColor="#ff010101"
        android:textSize="30dp" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/telephoneNumber"
        android:layout_alignTop="@+id/textView87"
        android:layout_alignLeft="@+id/textView83"
        android:layout_alignStart="@+id/textView83" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        android:id="@+id/button2"
        android:textColor="#ff000000"
        android:layout_below="@+id/textView87"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
